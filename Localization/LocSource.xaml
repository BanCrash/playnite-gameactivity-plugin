<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--
    THIS FILE IS NOT PART OF THE THEME, DO NOT MODIFY!
    -->

    <!-- 
    GameActivitySettingsView 
    -->

    <sys:String x:Key="LOCGameActivityActiveIcon">Show icon for launchers.</sys:String>
    
    <sys:String x:Key="LOCGameActivityActiveLog">Enable logging.</sys:String>
    <sys:String x:Key="LOCGameActivityTimeInterval">Time interval</sys:String>
    <sys:String x:Key="LOCGameActivityTimeLabel">minute(s)</sys:String>

    <sys:String x:Key="LOCGameActivityUseMsiAfterburner">Use MSI Afterburner</sys:String>
    <sys:String x:Key="LOCGameActivityUseHWiNFO">Use HWiNFO</sys:String>

    <sys:String x:Key="LOCGameActivitySensorsID">sensors id</sys:String>
    <sys:String x:Key="LOCGameActivityElementID">element id</sys:String>
    
    <sys:String x:Key="LOCGameActivityGpuUsage">GPU usage</sys:String>
    <sys:String x:Key="LOCGameActivityFps">FPS</sys:String>
    <sys:String x:Key="LOCGameActivityGpuTemp">GPU temperature</sys:String>
    <sys:String x:Key="LOCGameActivityCpuTemp">CPU temperature</sys:String>

    <sys:String x:Key="LOCGameActivityActiveWarning">Enable warnings.</sys:String>
    <sys:String x:Key="LOCGameActivityMinFps">Min FPS</sys:String>
    <sys:String x:Key="LOCGameActivityMaxCpuTemp">Max CPU temp</sys:String>
    <sys:String x:Key="LOCGameActivityMaxGpuTemp">Max GPU temp</sys:String>
    <sys:String x:Key="LOCGameActivityMaxCpuUsage">Max CPU usage</sys:String>
    <sys:String x:Key="LOCGameActivityMaxGpuUsage">Max GPU usage</sys:String>
    <sys:String x:Key="LOCGameActivityMaxRamUsage">Max RAM usage</sys:String>

    <sys:String x:Key="LOCGameActivityEnableLoggingToolTip">Log CPU, GPU, RAM and FPS performance during game session.</sys:String>
    <sys:String x:Key="LOCGameActivityEnableWarningToolTip">Displays a warning message at the end of the game session within the defined limits.</sys:String>


    <!-- 
    GameActivityView 
    -->
    <sys:String x:Key="LOCGameActivitySources">Sources</sys:String>
    <sys:String x:Key="LOCGameActivityGenres">Genres</sys:String>
    <sys:String x:Key="LOCGameActivityTotalHours">Total hours</sys:String>
    <sys:String x:Key="LOCGameActivityTotalHoursByWeeks">Total hours by weeks</sys:String>

    <sys:String x:Key="LOCGameActivityTime">Time</sys:String>
    <sys:String x:Key="LOCGameActivityLog">Log</sys:String>
    <sys:String x:Key="LOCGameActivityTimeTitle">Total hours by day</sys:String>
    <sys:String x:Key="LOCGameActivityLogTitle">Last session details</sys:String>

    <sys:String x:Key="LOCGameActivityLvGamesIcon">Icon</sys:String>
    <sys:String x:Key="LOCGameActivityLvGamesTitle">Name</sys:String>
    <sys:String x:Key="LOCGameActivityLvGamesLastActivity">Last session</sys:String>
    <sys:String x:Key="LOCGameActivityLvGamesElapsedSeconds">Elapsed time</sys:String>


    <!-- 
    GameActivity
    -->

    <sys:String x:Key="LOCGameActivityWarningCaption">Warnings during the game session</sys:String>
    <sys:String x:Key="LOCGameActivityWarningAt">At</sys:String>
    <sys:String x:Key="LOCGameActivityCpuUsage">CPU usage</sys:String>

</ResourceDictionary>
