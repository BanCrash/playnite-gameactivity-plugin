    <sys:String x:Key="LOCGameActivityGpuTemp">GPU temperature</sys:String>
    <sys:String x:Key="LOCGameActivityCpuTemp">CPU temperature</sys:String>


    <!-- 
    GameActivityView 
    -->
    <sys:String x:Key="LOCGameActivitySources">Sources</sys:String>
    <sys:String x:Key="LOCGameActivityGenres">Genres</sys:String>
    <sys:String x:Key="LOCGameActivityTotalHours">Total hours</sys:String>
    <sys:String x:Key="LOCGameActivityTotalHoursByWeeks">Total hours by weeks</sys:String>

    <sys:String x:Key="LOCGameActivityTime">Time</sys:String>
    <sys:String x:Key="LOCGameActivityLog">Log</sys:String>
    <sys:String x:Key="LOCGameActivityTimeTitle">Total hours by day</sys:String>
    <sys:String x:Key="LOCGameActivityLogTitle">Last session details"</sys:String>

    <sys:String x:Key="LOCGameActivityLvGamesIcon">Icon</sys:String>
    <sys:String x:Key="LOCGameActivityLvGamesTitle">Name</sys:String>
